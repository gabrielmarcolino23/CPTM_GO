# Global ignores for both backend and frontend
.DS_Store
Thumbs.db
.env
node_modules/
dist/
build/
tmp/
coverage/

# Specific to the backend (Node.js)
backend/node_modules/
backend/.env
backend/.cache
backend/logs/
backend/*.log
backend/.eslintcache
backend/.nyc_output

# Specific to the frontend (Angular)
frontend/node_modules/
frontend/dist/
frontend/.angular/cache/
frontend/.env
frontend/.eslintcache
frontend/src/**/*.spec.ts

# Ignore npm-debug.log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE/editor files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
*.iml
